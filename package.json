{
  "name": "library",
  "version": "0.0.1",
  "description": "My awesome library",
  "main": "dist/library.js",
  "module": "dist/library.esm.js",
  "types": "dist/index.d.ts",
  "source": "src",
  "buildFolder": "dist",
  "namespace": "Library",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
    "prettier": "prettier --write \"src/**/*.{js,ts}\"",
    "build:js": "rollup -c",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "build": "yarn build:types && yarn build:js",
    "build:watch": "rollup -c --watch",
    "postinstall": "yarn build",
    "examples": "http-server ./",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" yarn test"
  },
  "author": "Or Fleisher <https://orfleisher.com>",
  "license": "MIT",
  "homepage": "https://github.com/juniorxsound/TS-Library-Starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juniorxsound/TS-Library-Starter"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "add": "2.0.6",
    "chai": "^4.2.0",
    "eslint": "6.2.2",
    "eslint-config-prettier": "6.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "http-server": "^0.11.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "1.18.2",
    "rollup": "1.15.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-glslify": "^1.1.3",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.0.0",
    "terser": "4.0.0",
    "three": "^0.110.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.6.0"
  }
}
